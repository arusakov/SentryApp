{
  "extends": [
    "tslint:latest"
  ],
  "rules": {
    "trailing-comma": [
      true,  {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "semicolon": [
      true, "never"
    ],
    "quotemark": [
      true, "single"
    ],
    "member-access": false,
    "ordered-imports": [
      false
    ],
    "no-namespace": false,
    "interface-over-type-literal": false,
    "arrow-parens": false
  }
}